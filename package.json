{
	"name": "express-api-booky",
	"version": "1.2.0",
	"description": "An express API with a mongoDB databse for the projecr Booky",
	"main": "src/index.ts",
	"scripts": {
		"start": "ts-node src/index.ts || exit 0",
		"dev": "nodemon src/index.ts",
		"build": "tsc",
		"start:dist": "node dist/src/index.js",
		"test": "cd Backend-jest-GardenGuru && npm test",
		"ic:test": "concurrently --success first --kill-others \"npm run start\" \"sleep 5 && npm run test\"",
		"format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
		"format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
		"lint": "eslint --cache . --max-warnings=0",
		"lint:fix": "npm run lint -- --fix",
		"typecheck": "tsc --noEmit",
		"vercel-build": "echo hello"
	},
	"dependencies": {
		"axios": "^1.7.3",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"email-validator": "^2.0.4",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.17.0",
		"random-avatar-generator": "^2.0.0",
		"unique-names-generator": "^4.7.1",
		"uuidv7": "^1.0.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^20.12.6",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.14.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"globals": "^15.6.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.0",
		"prettier": "^3.3.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.4",
		"typescript-eslint": "^7.14.1"
	}
}
